{
    "name": "@ledes-facom/location-resolver",
    "version": "1.2.1",
    "main": "./lib/index.js",
    "types": "./lib/index.d.ts",
    "bin": {
        "location-resolver": "./lib/cli.js",
        "location-resolver-server": "./lib/server.js"
    },
    "license": "MIT",
    "repository": {
        "url": "https://github.com/ledes-facom/location-resolver.git"
    },
    "publishConfig": {
        "registry": "https://npm.pkg.github.com"
    },
    "author": {
        "name": "Hudson Silva Borges"
    },
    "scripts": {
        "build": "npm run lint && shx rm -rf lib && tsc && shx chmod -R +x ./lib",
        "dev": "ts-node -T ./src/cli.ts",
        "dev:server": "ts-node -T ./src/server.ts",
        "cli": "node ./lib/cli.js",
        "test": "jest ./src",
        "local": "npm i -g && location-resolver",
        "local:server": "npm i -g && location-resolver-server",
        "prepare": "is-ci || husky install || echo 'husky install skipped'",
        "release": "np --message 'chore: v%s'",
        "np": "npm rum release",
        "lint": "eslint ./src --ext .ts"
    },
    "dependencies": {
        "@keyv/sqlite": "^3.6.5",
        "axios": "^1.3.4",
        "bluebird": "^3.7.2",
        "cli-progress": "^3.12.0",
        "commander": "^10.0.0",
        "compression": "^1.7.4",
        "consola": "^2.15.3",
        "express": "^4.18.2",
        "fast-csv": "^4.3.6",
        "helmet": "^6.0.1",
        "keyv": "^4.5.2",
        "keyv-file": "^0.2.0",
        "load-balancers": "^1.3.52",
        "lodash": "^4.17.21",
        "p-queue": "^6"
    },
    "devDependencies": {
        "@commitlint/cli": "^17.5.0",
        "@commitlint/config-conventional": "^17.4.4",
        "@types/bluebird": "^3.5.38",
        "@types/cli-progress": "^3.11.0",
        "@types/compression": "^1.7.2",
        "@types/express": "^4.17.17",
        "@types/jest": "^29.5.0",
        "@types/lodash": "^4.14.191",
        "@types/node": "^18.15.7",
        "@types/shelljs": "^0.8.11",
        "@typescript-eslint/eslint-plugin": "^5.56.0",
        "@typescript-eslint/parser": "^5.56.0",
        "commitizen": "^4.3.0",
        "cz-conventional-changelog": "^3.3.0",
        "eslint": "^8.36.0",
        "eslint-config-prettier": "^8.8.0",
        "eslint-config-xo": "^0.43.1",
        "eslint-plugin-jest": "^27.2.1",
        "eslint-plugin-unused-imports": "^2.0.0",
        "husky": "^8.0.0",
        "is-ci": "^3.0.1",
        "jest": "^29.5.0",
        "np": "^7.6.4",
        "prettier": "^2.8.6",
        "shelljs": "^0.8.5",
        "shx": "^0.3.4",
        "tmp-promise": "^3.0.3",
        "ts-jest": "^29.0.5",
        "ts-node": "^10.9.1",
        "typescript": "^5.0.2"
    },
    "config": {
        "commitizen": {
            "path": "./node_modules/cz-conventional-changelog"
        }
    }
}
