{
    "name": "@ledes-facom/location-resolver",
    "version": "1.2.3",
    "main": "./lib/index.js",
    "types": "./lib/index.d.ts",
    "bin": {
        "location-resolver": "./lib/cli.js",
        "location-resolver-server": "./lib/server.js"
    },
    "license": "MIT",
    "repository": {
        "url": "https://github.com/ledes-facom/location-resolver.git"
    },
    "publishConfig": {
        "registry": "https://npm.pkg.github.com"
    },
    "author": {
        "name": "Hudson Silva Borges"
    },
    "scripts": {
        "build": "npm run lint && shx rm -rf lib && tsc && shx chmod -R +x ./lib",
        "dev": "ts-node -T ./src/cli.ts",
        "dev:server": "ts-node -T ./src/server.ts",
        "cli": "node ./lib/cli.js",
        "test": "jest ./src --coverage --collectCoverageFrom='src/**/*.ts'",
        "local": "npm i -g && location-resolver",
        "local:server": "npm i -g && location-resolver-server",
        "prepare": "is-ci || husky install || echo 'husky install skipped'",
        "release": "np --message 'chore: v%s'",
        "np": "npm rum release",
        "lint": "eslint ./src --ext .ts"
    },
    "dependencies": {
        "@keyv/sqlite": "^3.6.7",
        "axios": "^1.6.8",
        "bluebird": "^3.7.2",
        "cli-progress": "^3.12.0",
        "commander": "^12.0.0",
        "compression": "^1.7.4",
        "consola": "^3.2.3",
        "express": "^4.19.1",
        "fast-csv": "^5.0.1",
        "helmet": "^7.1.0",
        "keyv": "^4.5.4",
        "keyv-file": "^0.3.1",
        "load-balancers": "^1.3.52",
        "lodash": "^4.17.21",
        "p-queue": "^6"
    },
    "devDependencies": {
        "@commitlint/cli": "^19.2.1",
        "@commitlint/config-conventional": "^19.1.0",
        "@types/bluebird": "^3.5.42",
        "@types/cli-progress": "^3.11.5",
        "@types/compression": "^1.7.5",
        "@types/express": "^4.17.21",
        "@types/jest": "^29.5.12",
        "@types/lodash": "^4.17.0",
        "@types/node": "^20.11.30",
        "@types/shelljs": "^0.8.15",
        "@types/tmp": "^0.2.6",
        "@typescript-eslint/eslint-plugin": "^7.3.1",
        "@typescript-eslint/parser": "^7.3.1",
        "commitizen": "^4.3.0",
        "cz-conventional-changelog": "^3.3.0",
        "eslint": "^8.57.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-config-xo": "^0.44.0",
        "eslint-plugin-jest": "^27.9.0",
        "eslint-plugin-unused-imports": "^3.1.0",
        "husky": "^9.0.11",
        "is-ci": "^3.0.1",
        "jest": "^29.7.0",
        "np": "^10.0.1",
        "prettier": "^3.2.5",
        "shelljs": "^0.8.5",
        "shx": "^0.3.4",
        "tmp-promise": "^3.0.3",
        "ts-jest": "^29.1.2",
        "ts-node": "^10.9.2",
        "typescript": "^5.4.3"
    },
    "config": {
        "commitizen": {
            "path": "./node_modules/cz-conventional-changelog"
        }
    }
}
